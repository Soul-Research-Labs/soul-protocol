{
  "detectors_to_exclude": [
    "conformance-to-solidity-naming-conventions",
    "too-many-digits",
    "naming-convention",
    "dead-code",
    "solc-version",
    "timestamp",
    "constable-states"
  ],
  "exclude_informational": false,
  "exclude_low": false,
  "exclude_medium": false,
  "exclude_high": false,
  "exclude_dependencies": true,
  "filter_paths": "node_modules,lib,test,echidna,.certora_internal,forge-std,contracts/test,contracts/mocks,contracts/harness,contracts/testing",
  "solc_remaps": [
    "@openzeppelin/=node_modules/@openzeppelin/",
    "forge-std/=lib/forge-std/src/"
  ],
  "solc": "solc",
  "solc_args": "--via-ir --optimize --optimize-runs 10000",
  "compile_force_framework": "hardhat",
  "show_ignored_findings": false,
  "fail_on": "high",
  "legacy_ast": false
}
