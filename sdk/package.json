{
  "name": "@soul/sdk",
  "version": "1.0.0",
  "description": "Soul Protocol SDK - Cross-chain privacy middleware for Web3",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "soul": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./bridges": {
      "import": "./dist/esm/bridges/index.js",
      "require": "./dist/bridges/index.js",
      "types": "./dist/bridges/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/react/hooks.js",
      "require": "./dist/react/hooks.js",
      "types": "./dist/react/hooks.d.ts"
    },
    "./cli": {
      "import": "./dist/esm/cli/index.js",
      "require": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./compliance": {
      "import": "./dist/esm/compliance/SoulComplianceProvider.js",
      "require": "./dist/compliance/SoulComplianceProvider.js",
      "types": "./dist/compliance/SoulComplianceProvider.d.ts"
    },
    "./relayer": {
      "import": "./dist/esm/relayer/CrossChainProofRelayer.js",
      "require": "./dist/relayer/CrossChainProofRelayer.js",
      "types": "./dist/relayer/CrossChainProofRelayer.d.ts"
    },
    "./experimental": {
      "import": "./dist/esm/experimental/index.js",
      "require": "./dist/experimental/index.js",
      "types": "./dist/experimental/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "node test/sdk.test.mjs",
    "test:live": "node test/live.test.mjs",
    "test:all": "mocha -r ts-node/register test/**/*.ts",
    "test:coverage": "nyc mocha -r ts-node/register test/**/*.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "release": "npm publish --access public",
    "release:beta": "npm publish --access public --tag beta"
  },
  "keywords": [
    "privacy",
    "zk",
    "zero-knowledge",
    "bridge",
    "cross-chain",
    "ethereum",
    "web3",
    "l2",
    "noir",
    "stealth-addresses"
  ],
  "author": "Soul Team",
  "license": "MIT",
  "homepage": "https://github.com/Soul-Research-Labs/Soul#readme",
  "bugs": {
    "url": "https://github.com/Soul-Research-Labs/Soul/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soul-Research-Labs/Soul.git"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "poseidon-lite": "^0.3.0",
    "viem": "^2.30.0"
  },
  "optionalDependencies": {
    "@aztec/bb.js": "^0.82.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^8.0.2",
    "eslint": "^8.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "overrides": {
    "elliptic": "^6.6.1",
    "@ethersproject/signing-key": {
      "elliptic": "^6.6.1"
    }
  }
}
