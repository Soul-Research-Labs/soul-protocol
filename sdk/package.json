{
  "name": "@pil/sdk",
  "version": "1.0.0",
  "description": "Privacy Interoperability Layer SDK - Cross-chain privacy middleware with post-quantum cryptography for Web3",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "pil": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./bridges": {
      "import": "./dist/esm/bridges/index.js",
      "require": "./dist/bridges/index.js",
      "types": "./dist/bridges/index.d.ts"
    },
    "./pqc": {
      "import": "./dist/esm/pqc/index.js",
      "require": "./dist/pqc/index.js",
      "types": "./dist/pqc/index.d.ts"
    },
    "./react": {
      "import": "./dist/esm/react/hooks.js",
      "require": "./dist/react/hooks.js",
      "types": "./dist/react/hooks.d.ts"
    },
    "./zk": {
      "import": "./dist/esm/zkSystems/index.js",
      "require": "./dist/zkSystems/index.js",
      "types": "./dist/zkSystems/index.d.ts"
    },
    "./mpc": {
      "import": "./dist/esm/mpc/index.js",
      "require": "./dist/mpc/index.js",
      "types": "./dist/mpc/index.d.ts"
    },
    "./fhe": {
      "import": "./dist/esm/fhe/index.js",
      "require": "./dist/fhe/index.js",
      "types": "./dist/fhe/index.d.ts"
    },
    "./cli": {
      "import": "./dist/esm/cli/index.js",
      "require": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "test:coverage": "nyc mocha -r ts-node/register test/**/*.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "release": "npm publish --access public",
    "release:beta": "npm publish --access public --tag beta"
  },
  "keywords": [
    "privacy",
    "zk",
    "zero-knowledge",
    "bridge",
    "cross-chain",
    "cardano",
    "polkadot",
    "cosmos",
    "ethereum",
    "web3",
    "post-quantum",
    "pqc",
    "dilithium",
    "kyber",
    "sphincs",
    "hybrid-signatures",
    "quantum-resistant"
  ],
  "author": "PIL Team",
  "license": "MIT",
  "homepage": "https://github.com/pil-project/pil-sdk#readme",
  "bugs": {
    "url": "https://github.com/pil-project/pil-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pil-project/pil-sdk.git"
  },
  "dependencies": {
    "circomlibjs": "^0.1.7",
    "commander": "^11.0.0",
    "ethers": "^6.0.0",
    "ffjavascript": "^0.3.0",
    "snarkjs": "^0.7.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.9",
    "@types/snarkjs": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.0",
    "eslint": "^8.0.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "overrides": {
    "elliptic": "^6.6.1",
    "@ethersproject/signing-key": {
      "elliptic": "^6.6.1"
    }
  }
}
