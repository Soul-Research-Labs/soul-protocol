{
  "name": "@soul-midnight/bridge-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Soul-Midnight Bridge - Cross-chain privacy between Midnight Network and Ethereum/L2s",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./proof": {
      "import": "./dist/proof/index.mjs",
      "require": "./dist/proof/index.js",
      "types": "./dist/proof/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/proof/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "midnight",
    "ethereum",
    "bridge",
    "zk-proofs",
    "privacy",
    "cross-chain",
    "layer2",
    "arbitrum",
    "optimism",
    "base",
    "zksync",
    "scroll",
    "linea"
  ],
  "author": "Soul Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soul-protocol/midnight-bridge"
  },
  "homepage": "https://soulprotocol.dev/midnight-bridge",
  "bugs": {
    "url": "https://github.com/soul-protocol/midnight-bridge/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "viem": "^2.45.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  }
}
